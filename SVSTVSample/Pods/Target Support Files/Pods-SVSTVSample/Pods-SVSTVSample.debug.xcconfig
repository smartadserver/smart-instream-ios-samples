FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Smart-Core-TV-SDK" "${PODS_ROOT}/Smart-Instream-TV-SDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Smart-Core-TV-SDK" "${PODS_ROOT}/Headers/Public/Smart-Instream-TV-SDK"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Smart-Core-TV-SDK" -isystem "${PODS_ROOT}/Headers/Public/Smart-Instream-TV-SDK"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "SCSCoreKitTV" -framework "SVSVideoKitTV"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
